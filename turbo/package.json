{
	"name": "tttc-turbo",
	"version": "0.0.1",
	"scripts": {
		"dev": "npm run prepare && vite dev",
		"build": "npm run prepare && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write src",
		"prepare": "npm run smui-theme",
		"python-api": "java --add-opens java.base/java.util=ALL-UNNAMED -jar ../swagger-codegen-cli.jar generate -i static/swagger.json -l python -o src/python-api/",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"test-ui": "python3 src/lib/scripts/test.py && vitest --ui --open --coverage --passWithNoTests",
		"test": "vitest --coverage --run",
		"test-update-readme": "vitest --coverage --run --no-color --silent",
		"test-watch": "vitest --coverage",
		"test-help": "vitest -h"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
